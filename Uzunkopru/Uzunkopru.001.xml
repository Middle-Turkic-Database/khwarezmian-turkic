        <body>
            <div type="edition">
                <div type="textpart" subtype="chapter">
                    <ab>
                        <title></title>
                        <milestone unit="page" n="1r" />
                        <milestone unit="line" n="1" />
                        <lg type="couplet" n="1" xml:id="ON-001-001">
                            <l>qara ḫān ulusnï tüzätti bilib</l>
                            <l>ḫudānï unutub yaman iš qïlïb</l>
                        </lg>
                        <milestone unit="line" n="2" />
                        <lg type="couplet" n="2" xml:id="ON-001-002">
                            <l>iẕi rūzī qïldï bir oɣlan aŋa</l>
                            <l>nä kim qiṣṣa-larnï ayayïn saŋa</l>
                        </lg>
                        <milestone unit="line" n="3" />
                        <lg type="couplet" n="3" xml:id="ON-001-003">
                            <l>toɣub üč kün ötti ämüg ämmädi</l>
                            <l>anasïnïŋ harkiz südi tammadï</l>
                        </lg>
                        <milestone unit="line" n="4" />
                        <lg type="couplet" n="4" xml:id="ON-001-004">
                            <l>anasï qatïɣ qaẕɣu oɣlï üčün</l>
                            <l>bärib ämčäk anasï ne qïlsun küčün</l>
                        </lg>
                        <milestone unit="line" n="5" />
                        <lg type="couplet" n="5" xml:id="ON-001-005">
                            <l>tüšindä körär kim oɣlï aytu<addSpan spanTo="#ON-add001"/>r<anchor xml:id="ON-add001" type="addSpan" /> aŋa</l>
                            <l>ešitsäŋ anam bir sözüm bar saŋa</l>
                        </lg>
                        <milestone unit="line" n="6" />
                        <lg type="couplet" n="6" xml:id="ON-001-006">
                            <l>sen ol taŋrïɣa büt īmānɣa ulan</l>
                            <l>qoy ol bāṭil išni bu yanɣa ulan</l>
                        </lg>
                        <milestone unit="line" n="7" />
                        <lg type="couplet" n="7" xml:id="ON-001-007">
                            <l>ägär mundaɣ etsäŋ ämär män seni</l>
                            <l>yoq ärsä ölär män netärsän meni</l>
                        </lg>
                        <milestone unit="line" n="8" />
                        <lg type="couplet" n="8" xml:id="ON-001-008">
                            <l>anasï äšitti bu sözni turub</l>
                            <l>šukur qïldï ḥaqqa yüzini sürüb</l>
                        </lg>
                        <milestone unit="line" n="9" />
                        <lg type="couplet" n="9" xml:id="ON-001-009">
                            <l>čïnï birlä bütti yaratqanɣa ol</l>
                            <l>bu išdin tuyundï ne ḫōš toɣrï yol</l>
                        </lg>
                        <milestone unit="line" n="10" />
                        <lg type="couplet" n="10" xml:id="ON-001-010">
                            <l>ämä bašladï ol oɣul süt tiläb</l>
                            <l>anasï ämüzdi bašïnï siläb</l>
                        </lg>
                        <milestone unit="line" n="11" />
                        <lg type="couplet" n="11" xml:id="ON-001-011">
                            <l>bir yïl kečti oɣlan uluɣ boldï ḫōš</l>
                            <l>anasï sözindin qïlur erdi ǰōš</l>
                        </lg>
                        <milestone unit="line" n="12" />
                        <lg type="couplet" n="12" xml:id="ON-001-012">
                            <l>atasï qara ḫān sävä bašladï</l>
                            <l>köp elniŋ ičindä anï qašladï</l>
                        </lg>
                        <milestone unit="line" n="13" />
                        <lg type="couplet" n="13" xml:id="ON-001-013">
                            <l>ayur bir kün ärtä oɣul atïnï</l>
                            <l>demädük tiyäliŋ munuŋ ẕātïnï</l>
                        </lg>
                        <milestone unit="page" n="1v" />
                        <milestone unit="line" n="1" />
                        <lg type="couplet" n="14" xml:id="ON-002-001">
                            <l>mäšvärät qïlalïŋ öziŋä burun</l>
                            <l>ura käčäni yoqtur munuŋ täg yurun</l>
                        </lg>
                        <milestone unit="line" n="2" />
                        <lg type="couplet" n="15" xml:id="ON-002-002">
                            <l>mäniŋ urɣum ičrä munuŋ täg kiši</l>
                            <l>yoq erdi bu bolɣay ḫālayïk bašï</l>
                        </lg>
                        <milestone unit="line" n="3" />
                        <lg type="couplet" n="16" xml:id="ON-002-003">
                            <l>kim aš tarttï barča yedilär boɣuz</l>
                            <l>oɣul öz atïnï atadï oɣuz</l>
                        </lg>
